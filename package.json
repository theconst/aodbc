{
  "name": "cacheodbc",
  "version": "0.0.1",
  "description": "Cache ODBC - javascript library for ODBC access to Intersystems Cache",
  "dependencies": {
    "bindings": "^1.3.0"
  },
  "devDependencies": {
    "bluebird": "^3.5.2",
    "chai": "^4.1.2",
    "console.table": "^0.10.0",
    "mocha": "*",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "cmake-js": "^4.0.1",
    "nan": "^2.10.0"
  },
  "scripts": {
    "clean": "cmake-js clean",
    "compile": "cmake-js compile --release",
    "compile:debug": "cmake-js compile --debug",
    "test": "mocha test/**spec.js --reporter spec --full-trace --timeout 5000",
    "test:unit": "mocha test/**unit.spec.js --reporter spec --full-trace --timeout 5000",
    "test:integration": "mocha test/**integration.spec.js --reporter spec --full-trace --timeout 5000",
    "test:integration:statement": "mocha test/*statement.integration.spec.js --reporter spec --full-trace --timeout 5000",
    "test:integration:types": "mocha test/*types.integration.spec.js --reporter spec --full-trace --timeout 5000",
    "test:integration:transaction": "mocha test/*transaction.integration.spec.js --reporter spec --full-trace --timeout 5000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/TheConst/aodbc.git"
  },
  "keywords": [
    "ODBC",
    "Intersystems Cache"
  ],
  "author": "TheConst",
  "license": "MIT",
  "files": [
    "src/**",
    "index.js",
    "CMakeLists.txt",
    "package.json",
    "README.MD",
    "nanodbc/nanodbc/**",
    "nanodbc/CMakeLists.txt",
    "nanodbc/VERSION",
    "boost/variant/include/**",
    "boost/optional/include/**"
  ]
}
